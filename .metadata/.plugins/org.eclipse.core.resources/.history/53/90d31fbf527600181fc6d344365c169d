
public enum Gender { // Enum use for define variables with countable number
	Male(10 , "hello i'm styding java",15)
	,Female(21, "hello i'm waiting for new make up") ;
	
	int age ;
	String message ;
	
	private Gender(int age , String mes) {
		this.age = age ;
		this.message = mes ;
	}
}
